                <xsd:enumeration value="SM_CYDRAG">
                    <xsd:annotation>
                        <xsd:documentation>The number of pixels above and below a mouse-down point that the mouse pointer can move before a drag operation begins.</xsd:documentation>
                    </xsd:annotation>
