                </xsd:enumeration>
                <xsd:enumeration value="SM_CXMIN">
                    <xsd:annotation>
                        <xsd:documentation>The minimum width of a window, in pixels.</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="SM_CXMINIMIZED">
